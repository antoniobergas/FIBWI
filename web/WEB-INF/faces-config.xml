<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/ADMIN/admin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>revisarNoticiaAdmin</from-outcome>
            <to-view-id>/ADMIN/revisarNoticia.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/LOGIN/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/LOGIN/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logueadoComoAdmin</from-outcome>
            <to-view-id>/ADMIN/admin.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>logueadoComoAyto</from-outcome>
            <to-view-id>/AYTO/ayto.xhtml</to-view-id>
            <redirect/>
            
        </navigation-case>
        <navigation-case>
            <from-outcome>logueadoComoOtro</from-outcome>
            <to-view-id>/FRONT/front.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>panelRegistro</from-outcome>
            <to-view-id>/LOGIN/registro.xhtml</to-view-id>
            
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/LOGIN/registro.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>enviarALogin</from-outcome>
            <to-view-id>/LOGIN/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/AYTO/ayto.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cambiarContrasena</from-outcome>
            <to-view-id>/AYTO/cambiarpass.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/LOGIN/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>irFront</from-outcome>
            <to-view-id>/FRONT/front.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/AYTO/cambiarpass.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>moverRegreso</from-outcome>
            <to-view-id>/AYTO/ayto.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ADMIN/usuarios.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/LOGIN/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ADMIN/revisarNoticia.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cerrarSesion</from-outcome>
            <to-view-id>/LOGIN/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/FRONT/front.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>leerNoticia</from-outcome>
            <to-view-id>/FRONT/noticia.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>busquedaNoticia</from-outcome>
            <to-view-id>/FRONT/noticia.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
